{
  "name": "express-exit-intent",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "testServer": "jest --c jest.config.server.js --no-cache",
    "test": "npm run testServer && cd frontend && npm run test",
    "buildServer": "rm -rf dist && tsc -p tsconfig.server.json",
    "build": "npm run buildServer && cd frontend && npm run build",
    "postbuild": "if [ $(find src -type f -not -name '*.ts' | wc -l) -gt 0 ]; then cpy '**/*' '!**/*.ts' ../dist --cwd=src --no-overwrite --parents; fi",
    "start": "node dist/index.js -debug",
    "start:ts-node": "ts-node ./src/index.ts -debug",
    "inspect": "node --inspect=9777 -r ts-node/register ./src/index.ts -debug",
    "inspect-brk": "node --inspect-brk=9777 -r ts-node/register ./src/index.ts -debug",
    "commit": "git-cz",
    "lint:fix": "tslint --project tsconfig.server.json --fix",
    "semantic-release": "semantic-release --no-ci",
    "semantic-release:preview-only": "semantic-release --dry-run"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "keywords": [
    "TypeScript",
    "Express",
    "Jest"
  ],
